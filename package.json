{
	"name": "@tuplo/envsubst",
	"description": "Substitutes environment variables in strings",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/envsubst.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"envsubst",
		"environment",
		"variables"
	],
	"files": [
		"dist/index.mjs",
		"cjs/index.js",
		"dist/index.d.ts"
	],
	"types": "dist/index.d.ts",
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "tsx ./sh/build.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"coverage": "tsx ./sh/coverage.ts",
		"test:ci": "vitest run",
		"test": "vitest --watch"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.0",
		"@types/node": "20.4.2",
		"@typescript-eslint/eslint-plugin": "6.0.0",
		"@typescript-eslint/parser": "6.0.0",
		"@vitest/coverage-v8": "0.33.0",
		"esbuild": "0.18.12",
		"eslint": "8.44.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"nyc": "15.1.0",
		"prettier": "3.0.0",
		"tsx": "3.12.7",
		"typescript": "5.1.6",
		"vitest": "0.33.0"
	}
}
