{
  "name": "@tuplo/envsubst",
  "description": "Substitutes environment variables in strings",
  "version": "0.0.0-development",
  "repository": "git@github.com:tuplo/envsubst.git",
  "author": "Rui Costa",
  "license": "MIT",
  "keywords": [
    "envsubst",
    "environment",
    "variables"
  ],
  "files": [
    "dist/index.mjs",
    "cjs/index.js",
    "dist/index.d.ts"
  ],
  "main": "./cjs/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./cjs/index.js",
        "default": "./cjs/index.js"
      },
      "./dist/index.js"
    ]
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsx ./sh/build.ts",
    "format": "prettier --write src sh",
    "lint:ts": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "coverage": "tsx ./sh/coverage.ts",
    "test:ci": "jest --ci",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "esbuild": "0.15.16",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "nyc": "15.1.0",
    "prettier": "2.8.0",
    "ts-jest": "29.0.3",
    "tsx": "3.12.1",
    "typescript": "4.9.3",
    "zx": "7.1.1"
  }
}
